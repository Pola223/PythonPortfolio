signs = 'QAZXSWEDCVFRTGBNHYUJMKIOLPqazxswedcvfrtgbnhyujmkiolpĄąĆćĘęŻżŹźŃńŁłÓó1234567890!@#$%^&*()`~-_=+[{]}\'|;:"<>?/.,'

def random_password_generator(my_signs, length):
    password = "".join(random.sample(my_signs, length))
    return password
    
if __name__ == '__main__':
    random_password_generator(signs, length)
